<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
           http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">

    <changeSet id="9" author="Jared Lanoue">
        <loadUpdateData tableName="pokemon" schemaName="app_data" file="db/pokedex.csv" primaryKey="id">
            <column header="id" index="0" type="INT" name="id"/>
            <column header="name" index="1" type="TEXT" name="name"/>
            <column header="height" index="3" type="INT" name="height"/>
            <column header="weight" index="4" type="INT" name="weight" />
            <column header="genus" index="8" type="TEXT" name="genus" />
            <column header="description" index="9" type="TEXT" name="description"/>

            <column header="stats" type="skip"/>
            <column header="types" type="skip"/>
            <column header="abilities" type="skip"/>
            <column header="egg_groups" type="skip"/>
        </loadUpdateData>
    </changeSet>

